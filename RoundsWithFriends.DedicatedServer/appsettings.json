{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Server": {
    "Name": "RoundsWithFriends Dedicated Server",
    "Description": "Official RWF Server",
    "Port": 7777,
    "MaxPlayers": 16,
    "MaxTeams": 16,
    "TickRate": 60,
    "EnableConsole": true,
    "EnableWebAdmin": true,
    "WebAdminPort": 8080
  },
  "GameModes": {
    "Available": [
      "TeamDeathmatch",
      "Deathmatch"
    ],
    "Default": "TeamDeathmatch"
  },
  "Security": {
    "RequireAuthentication": false,
    "AdminPassword": "",
    "BanList": [],
    "Whitelist": []
  },
  "Performance": {
    "MaxConcurrentGames": 1,
    "HeartbeatInterval": 30000,
    "ConnectionTimeout": 60000
  }
}